
<launch>
	<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
		<param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />
		<rosparam file="$(find icarmulti_navigation)/config/move_base_params.yaml" command="load" />
		<rosparam file="$(find icarmulti_navigation)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
		<rosparam file="$(find icarmulti_navigation)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
		<rosparam file="$(find icarmulti_navigation)/config/local_costmap_params.yaml" command="load" />
		<rosparam file="$(find icarmulti_navigation)/config/global_costmap_params.yaml" command="load" />
		<rosparam file="$(find icarmulti_navigation)/config/base_local_planner_params.yaml" command="load" />
		<rosparam file="$(find icarmulti_navigation)/config/dwa_local_planner_params.yaml" command="load" />
	</node>
</launch>
